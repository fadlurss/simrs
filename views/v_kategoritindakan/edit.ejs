<% include ../partials/admin/header %>
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Edit Data Kategori Tindakan</h1>
        </div>

        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Edit Data Data Kategori Tindakan
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <form class="form-horizontal" role="form" action="/kategoritindakan/<%= kategoritindakan_edit_id._id %>?_method=PUT"
                        method="POST" id="tambah_barang">
                        <div class="form-group">
                            <label for="" class="col-sm-3">Kode Kategori Tindakan</label>
                            <div class="col-sm-9">
                                <input type="text" name="kode_kategori_tindakan" required class="form-control" value="<%= kategoritindakan_edit_id.kode_kategori_tindakan %>">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="" class="col-sm-3">Nama Penyakit</label>
                            <div class="col-sm-9">
                                <input type="text" name="kategori_tindakan" required class="form-control" value="<%= kategoritindakan_edit_id.kategori_tindakan %>">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-9 col-sm-offset-3">
                                <button class="btn btn-info btn-block">Update</button>
                            </div>
                        </div>
                    </form>

                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
</div>
<% include ../partials/admin/footer %>