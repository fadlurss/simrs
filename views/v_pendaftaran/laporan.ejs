<% include ../partials/admin/header %>
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Laporan Pendaftaran</h1>
            <br><br>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <div class="help-block">
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Data Pendaftaran
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body table-responsive">
                    <table width="100%" class="table table-striped table-bordered table-hover js-serial" id="datatable">
                        <thead>
                            <tr>
                                <th>No Reg</th>
                                <th>No Rawat</th>
                                <th>No RM</th>
                                <th>Nama pasien</th>
                                <th>Dokter</th>
                            </tr>
                        </thead>
                        <tbody>

                            <% cari_data.forEach(function(hasil){ %>
                            <tr>
                                <td>
                                    <%= hasil.no_registrasi %>
                                </td>
                                <td>
                                    <%= hasil.no_rawat %>
                                </td>
                                <td>
                                    <%= hasil.id_pasien.no_rm %>
                                </td>
                                <td>
                                    <%= hasil.id_pasien.nama_pasien %>
                                </td>
                                <td>
                                    <%= hasil.id_dokter_penanggung_jawab.nama_dokter %>
                                </td>

                            </tr>
                            <% }); %>
                        </tbody>
                    </table>

                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>

</div>
<!-- /#page-wrapper -->
<% include ../partials/admin/footer %>