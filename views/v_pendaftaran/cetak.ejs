<% include ../partials/admin/header_2 %>
<div class="col-lg-12">
    <h1 class="page-header">Detail Pendaftaran Pasien</h1>
</div>

<!-- /.col-lg-12 -->
</div>
<!-- /.row -->
<div class="row">


    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Biodata Pasien
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body table-responsive">
                <table width="100%" class="table table-striped table-bordered table-hover js-serial" id="datatable">
                    <tr>
                        <td width='200'>No Rawat</td>
                        <td>
                            <%= data_pendaftaran.no_rawat %>
                        </td>
                    </tr>
                    <tr>
                        <td width='200'>No Rekamedis</td>
                        <td>
                            <%= data_pendaftaran.id_pasien.no_rm %>
                        </td>
                    </tr>
                    <tr>
                        <td width='200'>Nama Pasien</td>
                        <td>
                            <%= data_pendaftaran.id_pasien.nama_pasien %>
                        </td>
                    </tr>
                    <tr>
                        <td width='200'>Riwayat Diagnosa Pakar Pasien</td>
                        <td>
                            <% data_pendaftaran.id_pasien.id_diagnosa_pakar.forEach(function(comment) { %>
                            <%= comment.nama_diagnosa %>
                            <% }) %>
                        </td>
                    </tr>
                </table>

            </div>

            <!-- /.panel-body -->
        </div>
    </div>




    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Riwayat diagnosa
            </div>
            <!-- /.panel-heading -->

            <div class="panel-body table-responsive">
                <table width="100%" class="table table-striped table-bordered table-hover js-serial" id="datatable">
                    <thead>
                        <tr>
                            <th>DS</th>
                            <th>DO</th>
                            <th>Keterangan</th>
                            <th>Tanggal</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% data_pendaftaran.id_riwayatdiagnosa.forEach(function(comment) { %>
                        <tr>
                            <td>
                                <%= comment.ds %>
                            </td>
                            <td>
                                <%= comment.do %>
                            </td>
                            <td>
                                <%= comment.keterangan %>
                            </td>
                            <td>
                                <%= moment(comment.tanggal).format('LL') %>
                            </td>
                        </tr>
                        <% }) %>
                    </tbody>

                </table>

            </div>
            <!-- /.panel-body -->
        </div>
    </div>


    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Riwayat periksa laboratorium
            </div>
            <!-- /.panel-heading -->

            <div class="panel-body table-responsive">
                <table width="100%" class="table table-striped table-bordered table-hover js-serial" id="datatable">
                    <thead>
                        <tr>
                            <th>Jenis pemeriksaan</th>
                            <th>Hasil pemeriksaan</th>
                            <th>Satuan</th>
                            <th>Nilai rujukan</th>
                            <th>Tanggal periksa</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% data_pendaftaran.id_riwayatdiagnosa.forEach(function(comment) { %>
                        <tr>
                            <td>
                                <%= comment.id_riwayat_periksa_lab.jenis_pemeriksaan %>
                            </td>
                            <td>
                                <%= comment.id_riwayat_periksa_lab.hasil_pemeriksaan %>
                            </td>
                            <td>
                                <%= comment.id_riwayat_periksa_lab.satuan %>
                            </td>
                            <td>
                                <%= comment.id_riwayat_periksa_lab.nilai_rujukan %>
                            </td>
                            <td>
                                <%= moment(comment.tanggal).format('LL') %>
                            </td>
                        </tr>
                        <% }) %>
                    </tbody>

                </table>

            </div>
            <!-- /.panel-body -->
        </div>
    </div>


    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Riwayat tindakan
            </div>
            <!-- /.panel-heading -->

            <div class="panel-body table-responsive">
                <table width="100%" class="table table-striped table-bordered table-hover js-serial" id="datatable">
                    <thead>
                        <tr>
                            <th>Tindakan</th>
                            <th>Hasil Periksa</th>
                            <th>Perkembangan</th>
                            <th>Tanggal</th>
                            <th>Tarif</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% data_pendaftaran.id_riwayattindakan.forEach(function(comment) { %>
                        <tr>
                            <td>
                                <%= comment.id_tindakan.nama_tindakan %>
                            </td>
                            <td>
                                <%= comment.hasil_periksa %>
                            </td>
                            <td>
                                <%= comment.perkembangan %>
                            </td>
                            <td>
                                <%= moment(comment.tanggal).format('LL') %>
                            </td>
                            <td id="tarif">
                                <%= comment.id_tindakan.tarif %>
                            </td>
                        </tr>
                        <% }) %>
                    </tbody>
                    <tr>
                        <td colspan="4" align="right">Total</td>
                        <td><b>
                                <%= total %></b></td>
                    </tr>
                </table>

            </div>
            <!-- /.panel-body -->
        </div>
    </div>

    <% include ../partials/admin/footer_2 %>

    <script>
        window.print();
    </script>