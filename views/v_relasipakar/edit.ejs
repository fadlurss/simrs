<% include ../partials/admin/header %>
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Tambah Data Relasi Pakar</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Tambah Relasi
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <form class="form-horizontal" role="form" action="/relasipakar/<%= relasipakar._id %>?_method=PUT"
                        method="POST" id="tambah_barang">
                        <div class="form-group">
                            <label for="" class="col-sm-3">Kode relasi</label>
                            <div class="col-sm-9">
                                <input type="text" name="kode_relasi_pakar" value="<%= relasipakar.kode_relasi_pakar %>"
                                    required class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="" class="col-sm-3">Kode diagnosa</label>
                            <div class="col-sm-9">
                                <select name="kode_diagnosa_pakar" id="" class="form-control" id="">
                                    <option value="" disabled>
                                        Pilihan sebelumnya :
                                        <%= relasipakar.kode_diagnosa_pakar %>
                                    </option>
                                    <% data_diagnosa.forEach(function(hasil){ %>
                                    <option value="<%= hasil.id %>">
                                        <%= hasil.kode_diagnosa %> -
                                        <%= hasil.nama_diagnosa %>
                                    </option>
                                    <% }); %>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="" class="col-sm-3">Kode gejala</label>
                            <div class="col-sm-9">
                                <select name="kode_gejala_pakar" id="" class="form-control" id="">
                                    <option value="" disabled>
                                        Pilihan sebelumnya :
                                        <%= relasipakar.kode_gejala_pakar %>
                                    </option>
                                    <% data_gejala.forEach(function(hasil){ %>
                                    <option value="<%= hasil.id %>">
                                        <%= hasil.kode_gejala %> -
                                        <%= hasil.nama_gejala %>
                                    </option>
                                    <% }); %>
                                </select>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-9 col-sm-offset-3">
                                <button class="btn btn-info btn-block">Save</button>
                            </div>
                        </div>
                    </form>

                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
</div>
<% include ../partials/admin/footer %>