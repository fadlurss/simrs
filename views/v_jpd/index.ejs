<% include ../partials/admin/header %>
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Data Jadwal Praktek Dokter</h1>
            <br><br>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <div class="help-block">
        <a href="/jadwalpraktek/new" class="btn btn-info">Tambah Jadwal Praktek Dokter baru</a>
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Jadwal Praktek Dokter Tables
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body table-responsive">
                    <table width="100%" class="table table-striped table-bordered table-hover" id="datatable">
                        <thead>
                            <tr>
                                <th>Nama dokter</th>
                                <th>Hari</th>
                                <th>Jam mulai</th>
                                <th>Jam selesai</th>
                                <th>Poli
                                </th>
                                <th width="120px">Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% data_jpd.forEach(function(hasil){ %>
                            <tr>
                                <td>
                                    <%= hasil.nama_dokter.nama_dokter %>
                                </td>
                                <td>
                                    <%= hasil.hari %>
                                </td>
                                <td>
                                    <%= hasil.jam_mulai %>
                                </td>
                                <td>
                                    <%= hasil.jam_selesai %>
                                </td>
                                <td>
                                    <%= hasil.poliklinik.nama_poliklinik %></a>
                                </td>
                                <td>
                                    <a class="btn btn-warning btn-sm" href="/jadwalpraktek/<%= hasil._id %>/edit"><i
                                            class="fa fa-pencil-square-o"></i></a>
                                    <a class="btn btn-white btn-sm" href="">
                                            <form action="/jadwalpraktek/<%= hasil._id %>?_method=DELETE" method="POST" class="delete-form">
                                                <button class="btn btn-danger btn-sm" onclick="return confirm('Apakah Kamu Yakin Ingin Menghapus Data <%= hasil.nama_dokter.nama_dokter  %> ?')"><i class="fa fa-trash-o"></i></button>
                                            </form>
                                    </a>
                                </td>
                            </tr>
                            <% }); %>
                        </tbody>
                    </table>

                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
</div>
<!-- /#page-wrapper -->
<% include ../partials/admin/footer %>