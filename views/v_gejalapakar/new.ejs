<% include ../partials/admin/header %>
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Tambah Data Gejala Pakar</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Tambah Gejala
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <form class="form-horizontal" role="form" action="/gejalapakar/new" method="POST"
                        id="tambah_diagnosapenyakit" enctype="multipart/form-data">
                        <div class="form-group">
                            <label for="" class="col-sm-3">Kode gejala</label>
                            <div class="col-sm-9">
                                <input type="text" name="kode_gejala" required class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="" class="col-sm-3">Nama gejala</label>
                            <div class="col-sm-9">
                                <input type="text" name="nama_gejala" required class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="" class="col-sm-3">Bobot</label>
                            <div class="col-sm-9">
                                <input type="text" name="bobot" class="form-control">
                            </div>
                        </div>
                        <label for="">Image</label>
                        <div class="form-group">
                            <!-- <input type="text" class="form-control" name="image" placeholder="Url of Image"> -->
                            <input type="file" id="image" class="form-control" name="image" accept="image/*" required>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-9 col-sm-offset-3">
                                <button class="btn btn-info btn-block">Save</button>
                            </div>
                        </div>
                    </form>

                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
</div>
<% include ../partials/admin/footer %>