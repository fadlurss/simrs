<% include ../partials/admin/header %>
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Data Sub Pemeriksaan Laboratorium</h1>
            <br><br>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <div class="help-block">
        <a href="/subpemeriksaanlaboratorium/<%= data_pemeriksaan_laboratorium._id %>/new" class="btn btn-info">Tambah
            Sub Pemeriksaan Laboratorium baru</a>
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Data Pemeriksaan Laboratorium
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body table-responsive">
                    <table width="100%" class="table table-striped table-bordered table-hover js-serial" id="datatable">
                        <tr>
                            <td width='200'>Kode Periksa</td>
                            <td>
                                <%= data_pemeriksaan_laboratorium.kode_periksa %>
                            </td>
                        </tr>
                        <tr>
                            <td width='200'>Nama Periksa</td>
                            <td>
                                <%= data_pemeriksaan_laboratorium.nama_periksa %>
                            </td>
                        </tr>
                        <tr>
                            <td width='200'>Tarif</td>
                            <td>
                                <%= data_pemeriksaan_laboratorium.tarif %>
                            </td>
                        </tr>
                    </table>

                </div>
                <!-- /.panel-body -->
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">
                    Sub Pemeriksaan Laboratorium Tables
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body table-responsive">
                    <table width="100%" class="table table-striped table-bordered table-hover js-serial" id="datatable">
                        <thead>
                            <tr>
                                <th>Kode Sub Periksa</th>
                                <th>Nama Pemeriksaan</th>
                                <th>Satuan</th>
                                <th>Nilai Rujukan</th>
                                <th width="120px">Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% data_sub_pemeriksaan_laboratorium.forEach(function(hasil){ %>
                            <tr>
                                <td>
                                    <%= hasil.kode_sub_periksa %>
                                </td>
                                <td>
                                    <%= hasil.nama_pemeriksaan %>
                                </td>
                                <td>
                                    <%= hasil.satuan %>
                                </td>
                                <td>
                                    <%= hasil.nilai_rujukan %>
                                </td>
                                <td>
                                    <a class="btn btn-warning btn-sm" href="/subpemeriksaanlaboratorium/<%= hasil._id %>/edit"><i
                                            class="fa fa-pencil-square-o"></i></a>
                                    <a class="btn btn-white btn-sm" href="">
                                        <form action="/subpemeriksaanlaboratorium/<%= hasil._id %>?_method=DELETE"
                                            method="POST" class="delete-form">
                                            <button class="btn btn-danger btn-sm" onclick="return confirm('Apakah Kamu Yakin Ingin Menghapus Data <%= hasil.nama_pemeriksaan %> ?')"><i
                                                    class="fa fa-trash-o"></i></button>
                                            <input type="text" hidden name="kode_periksa" value="<%= data_pemeriksaan_laboratorium._id %>">
                                        </form>
                                    </a>

                                </td>
                            </tr>
                            <% }); %>
                        </tbody>
                    </table>

                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
</div>
<!-- /#page-wrapper -->
<% include ../partials/admin/footer %>