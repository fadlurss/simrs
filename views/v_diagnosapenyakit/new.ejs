<% include ../partials/admin/header %>
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Tambah Data Diagnosa Penyakit</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Tambah Obat
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <form class="form-horizontal" role="form" action="/diagnosapenyakit/new" method="POST" id="tambah_diagnosapenyakit">
                        <div class="form-group">
                            <label for="" class="col-sm-3">Kode diagnosa</label>
                            <div class="col-sm-9">
                                <input type="text" name="kode_diagnosa" required class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="" class="col-sm-3">Nama penyakit</label>
                            <div class="col-sm-9">
                                <input type="text" name="nama_penyakit" required class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="" class="col-sm-3">Ciri-ciri penyakit</label>
                            <div class="col-sm-9">
                                <input type="text" name="ciri_ciri_penyakit" required class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="" class="col-sm-3">Keterangan</label>
                            <div class="col-sm-9">
                                <input type="text" name="keterangan" required class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="" class="col-sm-3">Ciri-ciri umum</label>
                            <div class="col-sm-9">
                                <input type="text" name="ciri_ciri_umum" required class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-9 col-sm-offset-3">
                                <button class="btn btn-info btn-block">Save</button>
                            </div>
                        </div>
                    </form>

                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
</div>
<% include ../partials/admin/footer %>