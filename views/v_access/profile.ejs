<% include ../partials/header2 %>
<div id="qbootstrap-services">
    <div class="container">
        <div class="row animate-box">
            <div class="col-md-6 col-md-offset-3">
                <h2 class="text-center">Biodata diri</h2>
                <p>No Rekamedis : <%= data_pasien.no_rm %></p>
                <p>Nama lengkap : <%= data_pasien.nama_pasien %> </p>
                <p>Tanggal lahir : <%= moment(data_pasien.tanggal_lahir).format('LL') %></p>
                <p>Umur : <%= data_pasien.umur %></p>
                <p>Alamat : <%= data_pasien.alamat %></p>
                <p>Pekerjaan : <%= data_pasien.pekerjaan %></p>
                <p>Jenis kelamin : <%= data_pasien.jenis_kelamin %></p>
                <br>

                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            Riwayat diagnosa dokter
                        </div>
                        <!-- /.panel-heading -->

                        <div class="panel-body table-responsive">
                            <table width="100%" class="table table-striped table-bordered table-hover js-serial"
                                id="datatable">
                                <thead>
                                    <tr>
                                        <th>DS</th>
                                        <th>DO</th>
                                        <th>Keterangan</th>
                                        <th>Tanggal</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% data_pendaftaran.id_riwayatdiagnosa.forEach(function(comment) { %>
                                    <tr>
                                        <td>
                                            <%= comment.ds %>
                                        </td>
                                        <td>
                                            <%= comment.do %>
                                        </td>
                                        <td>
                                            <%= comment.keterangan %>
                                        </td>
                                        <td>
                                            <%= moment(comment.tanggal).format('LL') %>
                                        </td>
                                    </tr>
                                    <% }) %>
                                </tbody>

                            </table>

                        </div>
                        <!-- /.panel-body -->
                    </div>
                </div>

                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            Riwayat periksa laboratorium
                        </div>
                        <!-- /.panel-heading -->

                        <div class="panel-body table-responsive">
                            <table width="100%" class="table table-striped table-bordered table-hover js-serial"
                                id="datatable">
                                <thead>
                                    <tr>
                                        <th>Jenis pemeriksaan</th>
                                        <th>Hasil pemeriksaan</th>
                                        <th>Satuan</th>
                                        <th>Nilai rujukan</th>
                                        <th>Tanggal periksa</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% data_pendaftaran.id_riwayatdiagnosa.forEach(function(comment) { %>
                                    <tr>
                                        <td>
                                            <%= comment.id_riwayat_periksa_lab.jenis_pemeriksaan %>
                                        </td>
                                        <td>
                                            <%= comment.id_riwayat_periksa_lab.hasil_pemeriksaan %>
                                        </td>
                                        <td>
                                            <%= comment.id_riwayat_periksa_lab.satuan %>
                                        </td>
                                        <td>
                                            <%= comment.id_riwayat_periksa_lab.nilai_rujukan %>
                                        </td>
                                        <td>
                                            <%= moment(comment.tanggal).format('LL') %>
                                        </td>
                                    </tr>
                                    <% }) %>
                                </tbody>

                            </table>

                        </div>
                        <!-- /.panel-body -->
                    </div>
                </div>


                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            Riwayat tindakan
                        </div>
                        <!-- /.panel-heading -->

                        <div class="panel-body table-responsive">
                            <table width="100%" class="table table-striped table-bordered table-hover js-serial"
                                id="datatable">
                                <thead>
                                    <tr>
                                        <th>Tindakan</th>
                                        <th>Hasil Periksa</th>
                                        <th>Perkembangan</th>
                                        <th>Tanggal</th>
                                        <th>Tarif</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% data_pendaftaran.id_riwayattindakan.forEach(function(comment) { %>
                                    <tr>
                                        <td>
                                            <%= comment.id_tindakan.nama_tindakan %>
                                        </td>
                                        <td>
                                            <%= comment.hasil_periksa %>
                                        </td>
                                        <td>
                                            <%= comment.perkembangan %>
                                        </td>
                                        <td>
                                            <%= moment(comment.tanggal).format('LL') %>
                                        </td>
                                        <td id="tarif">
                                            <%= comment.id_tindakan.tarif %>
                                        </td>
                                    </tr>
                                    <% }) %>
                                </tbody>

                            </table>

                        </div>
                        <!-- /.panel-body -->
                    </div>
                </div>

                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            Riwayat diagnosa pakar
                        </div>
                        <!-- /.panel-heading -->

                        <div class="panel-body table-responsive">
                            <table width="100%" class="table table-striped table-bordered table-hover js-serial"
                                id="datatable">
                                <thead>
                                    <tr>
                                        <th>Nama diagnosa</th>
                                        <th>Persentase</th>
                                        <th>Tanggal</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% data_riwayat_diagnosa.forEach(function(comment) { %>
                                    <tr>
                                        <td>
                                            <%= comment.nama_diagnosa %>
                                        </td>
                                        <td>
                                            <%= comment.persentansi %>
                                        </td>
                                        <td>
                                            <%= moment(comment.createdAt).format('LL') %>
                                        </td>
                                    </tr>
                                    <% }) %>
                                </tbody>

                            </table>

                        </div>
                        <!-- /.panel-body -->
                    </div>
                </div>


            </div>
        </div>
    </div>
</div>

<% include ../partials/footer %>